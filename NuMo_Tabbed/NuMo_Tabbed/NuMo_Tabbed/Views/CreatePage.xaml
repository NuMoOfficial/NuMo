<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NuMo_Tabbed.Views.CreatePage"
             Title="Create Custom Foods and Recipes">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Create Food" Clicked="AddCreateFood" Margin="0, 5.5, 0, 0" Style="{StaticResource buttonStyle}"/>
            <Button Text="Create Recipe" Clicked="AddCreateRecipe" Style="{StaticResource buttonStyle}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>